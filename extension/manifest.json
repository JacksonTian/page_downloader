{
  "manifest_version": 3,
  "name": "Page Downloader",
  "version": "1.1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon16.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}